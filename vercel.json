{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/save-appointment.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/retrieve-appointment-details.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/save-appointment",
        "dest": "/api/save-appointment.js"
      },
      {
        "src": "/api/retrieve-appointment-details",
        "dest": "/api/retrieve-appointment-details.js"
      }
    ]
  }
  